{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-adf-project-rimi"
		},
		"external_azure_container_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'external_azure_container'"
		},
		"linkedService_Azure_Datalake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'linkedService_Azure_Datalake'"
		},
		"linkedService_Azure_SQL_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'linkedService_Azure_SQL'"
		},
		"rg-synapse-workspace-WorkspaceDefaultSqlServer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'rg-synapse-workspace-WorkspaceDefaultSqlServer'",
			"defaultValue": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=tcp:rg-synapse-workspace.sql.azuresynapse.net,1433;Initial Catalog=@{linkedService().DBName}"
		},
		"external_azure_container_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sargsynapse.dfs.core.windows.net/"
		},
		"linkedService_Azure_Datalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://saadfprojectrimi.dfs.core.windows.net/"
		},
		"linkedService_Azure_SQL_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqlserver-rg-adf-project.database.windows.net"
		},
		"linkedService_Azure_SQL_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sqldb-rg-adf-project"
		},
		"linkedService_Azure_SQL_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "dippybiswas"
		},
		"rg-synapse-workspace-WorkspaceDefaultStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sasynapsedefault.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-adf-project-rimi_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"linkedService_Azure_Datalake_accountKey": {
						"value": "[parameters('linkedService_Azure_Datalake_accountKey')]"
					},
					"linkedService_Azure_SQL_password": {
						"value": "[parameters('linkedService_Azure_SQL_password')]"
					},
					"external_azure_container_accountKey": {
						"value": "[parameters('external_azure_container_accountKey')]"
					},
					"rg-synapse-workspace-WorkspaceDefaultSqlServer_connectionString": {
						"value": "[parameters('rg-synapse-workspace-WorkspaceDefaultSqlServer_connectionString')]"
					},
					"linkedService_Azure_Datalake_properties_typeProperties_url": {
						"value": "[parameters('linkedService_Azure_Datalake_properties_typeProperties_url')]"
					},
					"linkedService_Azure_SQL_properties_typeProperties_server": {
						"value": "[parameters('linkedService_Azure_SQL_properties_typeProperties_server')]"
					},
					"linkedService_Azure_SQL_properties_typeProperties_database": {
						"value": "[parameters('linkedService_Azure_SQL_properties_typeProperties_database')]"
					},
					"linkedService_Azure_SQL_properties_typeProperties_userName": {
						"value": "[parameters('linkedService_Azure_SQL_properties_typeProperties_userName')]"
					},
					"external_azure_container_properties_typeProperties_url": {
						"value": "[parameters('external_azure_container_properties_typeProperties_url')]"
					},
					"rg-synapse-workspace-WorkspaceDefaultStorage_properties_typeProperties_url": {
						"value": "[parameters('rg-synapse-workspace-WorkspaceDefaultStorage_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}