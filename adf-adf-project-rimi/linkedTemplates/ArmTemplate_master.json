{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-adf-project-rimi"
		},
		"linkedService_Azure_Datalake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'linkedService_Azure_Datalake'"
		},
		"linkedService_Azure_SQL_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'linkedService_Azure_SQL'"
		},
		"linkedService_Azure_Datalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://saadfprojectrimi.dfs.core.windows.net/"
		},
		"linkedService_Azure_SQL_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqlserver-rg-adf-project.database.windows.net"
		},
		"linkedService_Azure_SQL_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sqldb-rg-adf-project"
		},
		"linkedService_Azure_SQL_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "dippybiswas"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-adf-project-rimi_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"linkedService_Azure_Datalake_accountKey": {
						"value": "[parameters('linkedService_Azure_Datalake_accountKey')]"
					},
					"linkedService_Azure_SQL_password": {
						"value": "[parameters('linkedService_Azure_SQL_password')]"
					},
					"linkedService_Azure_Datalake_properties_typeProperties_url": {
						"value": "[parameters('linkedService_Azure_Datalake_properties_typeProperties_url')]"
					},
					"linkedService_Azure_SQL_properties_typeProperties_server": {
						"value": "[parameters('linkedService_Azure_SQL_properties_typeProperties_server')]"
					},
					"linkedService_Azure_SQL_properties_typeProperties_database": {
						"value": "[parameters('linkedService_Azure_SQL_properties_typeProperties_database')]"
					},
					"linkedService_Azure_SQL_properties_typeProperties_userName": {
						"value": "[parameters('linkedService_Azure_SQL_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}